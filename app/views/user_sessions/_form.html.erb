<%= bootstrap_form_for @user_session, inline_errors: false do |f| %>
  <%= f.alert_message "Whoops!" %>
  <%= f.text_field :username, hide_label: true, placeholder: 'Username' %>
  <%= f.password_field :password, hide_label: true, placeholder: 'Password' %>
  <%= f.submit 'Login', class: "btn btn-primary btn-lg btn-block" %>
<% end %>